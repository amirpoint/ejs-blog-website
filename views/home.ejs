<%- include("partials/header") %> 
    <h1>Home</h1>
    <p><%= homeParagraph %> </p>

    <% posts.forEach(function (post) { %>
        <h1><%= post.title %></h1>
        <p><%= `${post.content.slice(0, 100)}... ` %> <a href="posts/<%= post.title %>" style="font-size: 20px;">↝</a></p>
    <% }); %> 

<%- include("partials/footer") %>